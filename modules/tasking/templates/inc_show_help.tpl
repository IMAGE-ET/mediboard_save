<table class="tbl">
  <tr>
    <th>^</th>
    <td>{{tr}}CTaskingTicket-smart-add-due-date{{/tr}} (dd/mm/yyyy)</td>
  </tr>
  <tr>
    <th>!</th>
    <td>{{tr}}CTaskingTicket-smart-add-priority{{/tr}}</td>
  </tr>
  <tr>
    <th>@</th>
    <td>{{tr}}CTaskingTicket-smart-add-type{{/tr}} (fnc | ref | bug | erg | action)</td>
  </tr>
  <tr>
    <th>#</th>
    <td>{{tr}}CTaskingTicket-smart-add-tags{{/tr}}</td>
  </tr>
