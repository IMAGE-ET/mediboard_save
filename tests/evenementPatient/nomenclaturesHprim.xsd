   <!--
      "remiseResultatAvancementHprim" :
      Nomenclature des avancements de résultats pour lesquels le demandeur souhaite recevoir les résultats
      ces 4 valeurs sont combinables en une chaine.
      Exemple : ivf : acquittement des échantillons + résultats partiels vérifiés + résultat final
      Source : commission technique H'.
         Code     Traduction
         .........      ..............................................
         i        initial (aucun résultat encore obtenu = acquittement des échantillons)
         p        partiel non vérifié
         v        partiel vérifié
         f        final
   -->
   <xsd:simpleType name="remiseResultatAvancementHprim">
      <xsd:restriction base="xsd:string">
         <xsd:pattern value="(i?p?v?f?)"/>
      </xsd:restriction>
   </xsd:simpleType>
   <!--
      "remiseResultatContenuHprim" :
      Nomenclature des contenus de résultats que le demandeur souhaite recevoir
      ces 3 valeurs sont combinables en une chaine.
      Exemple : stj : résultats structurés + texte + pièces jointes
      Source : commission technique H'.
         Code     Traduction
         .........      ..............................................
         s        résultats structurés
         t        texte
         j        pièces jointes
   -->
   <xsd:simpleType name="remiseResultatContenuHprim">
      <xsd:restriction base="xsd:string">
         <xsd:pattern value="(s?t?j?)"/>
      </xsd:restriction>
   </xsd:simpleType>
   <!--
      "remiseResultatSupportHprim" :
