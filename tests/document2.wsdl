  </types>
  <message name="getHelloRequest">
    <part name="prenom" type="xsd:string"/>
    <part name="nom" type="xsd:string"/>
  </message>
  <message name="getHelloResponse">
    <part name="return" type="xsd:string"/>
  </message>
  
  <message name="enregistrementPatientRequest">
    <part name="login" type="xsd:string"/>
    <part name="password" type="xsd:string"/>
    <part name="message" type="xsd:anyType"/>
  </message>
  <message name="enregistrementPatientResponse">
    <part name="return" type="xsd:string"/>
  </message>
  
  <portType name="MediboardPort">
    <operation name="getHello">
      <input message="typens:getHelloRequest"/>
      <output message="typens:getHelloResponse"/>
    </operation>
    <operation name="enregistrementPatient">
      <input message="typens:enregistrementPatientRequest"/>
      <output message="typens:enregistrementPatientResponse"/>
    </operation>
  </portType>
  
  <binding name="MediboardBinding" type="typens:MediboardPort">
    <soap:binding xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" style="rpc" transport="http://schemas.xmlsoap.org/soap/http"/>
    <operation name="getHello">
      <soap:operation xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" soapAction="MediboardAction"/>
      <input name="getHelloRequest">
        <soap:body xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" use="encoded" namespace="urn:MediboardWSDL" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
      </input>
      <output name="getHelloReponse">
        <soap:body xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" use="encoded" namespace="urn:MediboardWSDL" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
      </output>
    </operation>
    <operation name="enregistrementPatient">
      <soap:operation xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" soapAction="MediboardAction"/>
      <input name="enregistrementPatientRequest">
        <soap:body xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" use="encoded" namespace="urn:MediboardWSDL" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
      </input>
      <output name="enregistrementPatientReponse">
        <soap:body xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" use="encoded" namespace="urn:MediboardWSDL" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
