         <xsd:enumeration value="prt"/>
         <xsd:enumeration value="chr"/>
         <xsd:enumeration value="fax"/>
         <xsd:enumeration value="ml"/>
         <xsd:enumeration value="hx"/>
         <xsd:enumeration value="tel"/>
      </xsd:restriction>
   </xsd:simpleType>
