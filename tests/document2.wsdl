    </operation>
  </binding>
  <service name="MediboardService">
    <documentation>Ceci est une documentation du WebService</documentation>
    <port name="MediboardPort" binding="MediboardBinding">
      <soap:address xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" location="http://192.168.0.101/~yohann/mediboard_yohann/index.php?soap=1"/>
    </port>
  </service>
